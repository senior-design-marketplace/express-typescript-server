{
	"name": "express-typescript-server",
	"version": "1.0.0",
	"description": "",
	"files": [
		"build"
	],
	"main": "lambda.js",
	"scripts": {
		"tsc": "ttsc",
		"test:integ:local": "cross-env NODE_ENV=test npm run test:integ:child",
		"test:integ:stage": "cross-env NODE_ENV=staging npm run test:integ:child",
		"test:integ:child": "npm run db:export:child && knex seed:run && ttsc && npm run claudia:build && jest -c jest.config.integ.js --forceExit",
		"test:unit": "jest -c jest.config.unit.js",
		"types:generate": "ts-node core/src/types/generate",
		"claudia:build": "claudia generate-serverless-express-proxy --config core/claudia.json --express-module build/core/src/app",
		"claudia:build:mediaHandler": "claudia update --config external/mediaHandler/claudia.json",
		"claudia:deploy:stage": "claudia update --config core/claudia.json --version staging",
		"claudia:promote:production": "claudia set-version --config core/claudia.json --version production",
		"db:export:local": "cross-env NODE_ENV=test npm run db:export:child",
		"db:export:stage": "cross-env NODE_ENV=staging npm run db:export:child",
		"db:export:child": "ts-node scripts/export",
		"db:deploy:local": "knex migrate:rollback --env test && knex migrate:latest --env test",
		"db:deploy:stage": "knex migrate:rollback --env staging && knex migrate:latest --env staging",
		"db:seed:local": "knex seed:run --env test",
		"db:seed:stage": "knex seed:run --env staging"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/senior-design-marketplace/express-typescript-server.git"
	},
	"author": "Mark Freeman",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/senior-design-marketplace/express-typescript-server/issues"
	},
	"homepage": "https://github.com/senior-design-marketplace/express-typescript-server#readme",
	"dependencies": {
		"@overnightjs/core": "^1.6.9",
		"@overnightjs/logger": "^1.1.9",
		"ajv": "^6.10.2",
		"aws-sdk": "^2.551.0",
		"aws-serverless-express": "^3.3.6",
		"body-parser": "^1.19.0",
		"cookie-parser": "^1.4.4",
		"cors": "^2.8.5",
		"cross-env": "^6.0.3",
		"express": "^4.17.1",
		"express-async-handler": "^1.1.4",
		"express-query-boolean": "^2.0.0",
		"http-status-codes": "^1.3.2",
		"json-schema-ref-parser": "^7.1.1",
		"jsonwebtoken": "^8.5.1",
		"jwk-to-pem": "^2.0.3",
		"knex": "^0.19.5",
		"lodash": "^4.17.19",
		"objection": "^2.1.2",
		"pg": "^7.12.1",
		"require-from-string": "^2.0.2",
		"ts-custom-error": "^3.1.1",
		"ts-transformer-keys": "^0.3.5",
		"uuid": "^3.3.3",
		"validator": "^12.2.0"
	},
	"devDependencies": {
		"@types/aws-lambda": "^8.10.46",
		"@types/aws-serverless-express": "^3.3.1",
		"@types/body-parser": "^1.17.1",
		"@types/cors": "^2.8.6",
		"@types/crypto-js": "^3.1.43",
		"@types/express": "^4.17.1",
		"@types/fs-extra": "^8.0.1",
		"@types/jest": "^24.0.19",
