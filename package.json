		"@types/pg": "^7.14.0",
		"@types/require-from-string": "^1.2.0",
		"@types/sinon": "^7.5.0",
		"@types/sinon-express-mock": "^1.3.7",
		"ajv-merge-patch": "^4.1.0",
		"ajv-pack": "^0.3.1",
		"claudia": "^5.9.0",
		"crypto-js": "^3.1.9-1",
		"faker": "^4.1.0",
