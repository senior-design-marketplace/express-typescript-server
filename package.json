{
	"name": "express-typescript-server",
	"version": "1.0.0",
	"description": "",
	"files": [
		"build"
	],
	"main": "lambda.js",
	"scripts": {
		"tsc": "ttsc",
		"prettier": "prettier --write './**/*.*' --ignore-path ./.prettierignore --use-tabs true",
		"test:integ:local": "cross-env NODE_ENV=test npm run test:integ:child",
		"test:integ:stage": "cross-env NODE_ENV=staging npm run test:integ:child",
		"test:integ:child": "npm run db:export:child && ttsc && npm run claudia:build && jest -c jest.config.integ.js --forceExit",
		"test:unit": "jest -c jest.config.unit.js",
		"schema:bundle": "ts-node src/schemas/bundle",
		"claudia:build": "claudia generate-serverless-express-proxy --express-module build/app",
		"claudia:deploy:stage": "claudia update --version staging",
		"claudia:promote:production": "claudia set-version --version production",
		"db:export:local": "cross-env NODE_ENV=test npm run db:export:child",
		"db:export:stage": "cross-env NODE_ENV=staging npm run db:export:child",
		"db:export:child": "ts-node src/access/export",
		"db:deploy:local": "knex migrate:rollback --env test && knex migrate:latest --env test",
		"db:deploy:stage": "knex migrate:rollback --env staging && knex migrate:latest --env staging",
		"db:seed:local": "knex seed:run --env test",
		"db:seed:stage": "knex seed:run --env staging"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/senior-design-marketplace/express-typescript-server.git"
	},
	"author": "Mark Freeman",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/senior-design-marketplace/express-typescript-server/issues"
	},
	"homepage": "https://github.com/senior-design-marketplace/express-typescript-server#readme",
	"dependencies": {
		"@overnightjs/core": "^1.6.9",
		"@overnightjs/logger": "^1.1.9",
		"ajv": "^6.10.2",
		"aws-sdk": "^2.551.0",
		"aws-serverless-express": "^3.3.6",
		"body-parser": "^1.19.0",
		"cookie-parser": "^1.4.4",
		"cors": "^2.8.5",
		"cross-env": "^6.0.3",
		"express": "^4.17.1",
		"express-async-handler": "^1.1.4",
		"http-status-codes": "^1.3.2",
		"json-schema-ref-parser": "^7.1.1",
		"knex": "^0.19.5",
		"lodash": "^4.17.15",
		"objection": "^1.6.11",
		"pg": "^7.12.1",
		"ts-custom-error": "^3.1.1",
		"ts-transformer-keys": "^0.3.5",
		"uuid": "^3.3.3"
	},
	"devDependencies": {
		"@types/aws-serverless-express": "^3.3.1",
		"@types/body-parser": "^1.17.1",
		"@types/cors": "^2.8.6",
		"@types/express": "^4.17.1",
		"@types/fs-extra": "^8.0.1",
		"@types/jest": "^24.0.19",
		"@types/lodash": "^4.14.144",
		"@types/node": "^12.11.1",
		"@types/sinon": "^7.5.0",
		"@types/sinon-express-mock": "^1.3.7",
		"ajv-merge-patch": "^4.1.0",
		"ajv-pack": "^0.3.1",
		"claudia": "^5.9.0",
		"faker": "^4.1.0",
		"fs-extra": "^8.1.0",
		"jest": "^24.9.0",
		"json-schema-to-typescript": "^7.1.0",
		"prettier": "^1.18.2",
		"sinon": "^7.5.0",
		"sinon-express-mock": "^2.2.1",
		"sqlite3": "^4.1.0",
		"ts-jest": "^24.1.0",
		"ts-node": "^8.4.1",
		"ttypescript": "^1.5.7",
		"typescript": "3.7.0-beta"
	}
}
