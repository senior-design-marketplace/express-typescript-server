{
	"title": "Description Attributes",
	"description": "A set of descriptors for a given project",
	"type": "object",
	"properties": {
		"photos": {
			"description": "Links to S3 files of photographs for this project",
			"type": "array",
			"items": {
				"type": "string",
				"pattern": "(?=^.{3,63}$)(?!^(\\d+\\.)+\\d+$)(^(([a-z0-9]|[a-z0-9][a-z0-9\\-]*[a-z0-9])\\.)*([a-z0-9]|[a-z0-9][a-z0-9\\-]*[a-z0-9])$)"
			}
		},
		"comments": {
			"description": "Discussion surrounding the project",
			"type": "array",
			"items": {
				"$ref": "comment.json#"
			}
		}
	}
}
